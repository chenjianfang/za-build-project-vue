{
    "name": "@za-build/project-vue",
    "version": "1.1.7",
    "description": "vue构建脚手架",
    "scripts": {
        "dev": "webpack-dev-server --inline --color --host 0.0.0.0 --disable-host-check --config build/webpack.dev.conf.js",
        "prod": "cross-env NODE_ENV=production webpack --process --color --config build/webpack.prod.conf.js",
        "analyzer": "webpack --config ./build/webpack.prod.conf.js --profile --json > stats.json && webpack-bundle-analyzer stats.json -m static",
        "lint": "eslint --ext .js,.vue src",
        "lintfix": "eslint --fix --ext .js,.vue src"
    },
    "bin": {
        "za-dev-vue": "./bin/dev.js",
        "za-prod-vue": "./bin/prod.js",
        "za-eject-vue": "./bin/eject.js"
    },
    "keywords": [
        "vue",
        "build",
        "ZA"
    ],
    "author": "ZAFE",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenjianfang/za-build-project-vue"
    },
    "files": [
        "bin",
        "build",
        "config",
        ".gitignore",
        ".postcssrc.js",
        "babel.config.json",
        "package.json",
        "readme.md"
    ],
    "dependencies": {
        "@babel/runtime": "^7.11.2",
        "@babel/core": "^7.11.1",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-stage-2": "^7.8.3",
        "autoprefixer-loader": "^3.2.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "chalk": "^2.0.1",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^7.0.2",
        "css-loader": "^0.28.4",
        "cssnano": "^3.10.0",
        "eslint": "^7.10.0",
        "vue-loader": "15.3.0",
        "vue-style-loader": "3.0.1",
        "vue-template-compiler": "2.5.21",
        "eslint-plugin-vue": "^6.2.2",
        "eslint-webpack-plugin": "^2.1.0",
        "file-loader": "^1.1.11",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "html-loader": "^0.5.0",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "latest",
        "node-notifier": "^5.2.1",
        "node-sass": "^4.5.3",
        "optimize-css-assets-webpack-plugin": "^3.0.0",
        "portfinder": "^1.0.28",
        "postcss-loader": "^2.0.6",
        "postcss-px2rem": "^0.3.0",
        "sass-loader": "^6.0.6",
        "shelljs": "^0.8.4",
        "style-loader": "^0.18.2",
        "url-loader": "^0.5.9",
        "webpack": "^4.5.0",
        "webpack-cli": "^3.1.1",
        "webpack-dev-server": "^3.1.3",
        "webpack-hot-middleware": "^2.21.2",
        "webpack-manifest-plugin": "^2.2.0",
        "webpack-merge": "^4.1.2"
    },
    "peerDependencies": {
        "vue": "2.5.21",
        "core-js": "^3.6.5"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
